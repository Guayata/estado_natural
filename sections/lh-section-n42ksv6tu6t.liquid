		            	</div>-->
		            	 
							<div class="lh-rating-wrapper">
								{% render 'layouthub_review',  review_type: 'rating' ,product: product %}
							</div>
						 
		                <p class="lh-product-title">
		                    <a class="" href="{{product.url | with: collection }}">{{ product.title}}</a>
						</p>
										
						<div class="lh-wrap-swap">
							<div class="lh-wrap-price">
								<div class="lh-price-box">
									
									{% if isSale  %}
										<ins class="lh-product-price">{{ first_variant.price | money }}</ins>
										<del class="lh-product-compare-price">{{ first_variant.compare_at_price | money  }}</del>
										
									{%- else -%}
										<!--<ins class="lh-product-price">{{ first_variant.price | money }}</ins>
										<del class="lh-product-compare-price">{{ first_variant.compare_at_price | money  }}</del>-->
										
										<span class="lh-product-price a">{{ first_variant.price | money }}</span>
									{% endif %}
									
								</div>
							</div>
						</div>
						
					
						<div class="lh-button">
							{% if  product.variants.size < 2 and product.available  %}
