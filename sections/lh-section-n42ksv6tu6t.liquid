    <div class="js-testimonials-list swiper-wrapper ">
      {% for product in collection.products limit: 10%}
						{% assign first_variant = product.selected_or_first_available_variant %}
						{% assign v_type = '' %}
						{% assign isSale = false %}
						{%- if first_variant.available -%}
							{%- if first_variant.compare_at_price > first_variant.price -%}
								{% assign isSale = true %}
								{% assign v_type = 'sale' %}
							{%- else if -%}
								{% assign v_type = 'no_sale' %}
							{%- endif -%}
						{%- else -%}
							{% assign v_type = 'soldout' %}
						{%- endif -%}
		        <div class="lh-product-item swiper-slide" data-id="{{product.id}}" data-v-type="{{v_type}}">
					<div  class="lh-product-item-images">
        					<a class="lh-product-item-image" href="{{product.url | within: collection}}">
								 
									<img src="{{ product.featured_image | img_url: '270x360_crop_center' }}" alt="{{product.title}}">
									{% if product.images.size > 1 %}
										<img src="{{product.images[1] | img_url : '270x360_crop_center' }}" class="lh-img-hover" alt="{{product.title}}">
									{% else %}
										<img src="{{product.featured_image | img_url: '270x360_crop_center' }}" class="lh-img-hover" alt="{{product.title}}">
									{% endif %}
								 
							</a>
							<img class="lh-img-loading" src="{{'n42ksv6tu6t_loading.gif' | asset_url }}" alt="loading">
