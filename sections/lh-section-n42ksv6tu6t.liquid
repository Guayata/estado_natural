								<!--<span class="lh-sale-off lh-sale-text"  data-lhi="label_sale" data-lh-lang></span>-->
								<span class="lh-sale-off lh-sale-num">
								   -{{ first_variant.compare_at_price | minus: first_variant.price | times: 100 | divided_by: first_variant.compare_at_price| round: 2}}%
								</span>
								{%- for tag in product.tags -%}
									{%- if tag == 'new' or tag == 'New' -%}
										<span class="lh-hot"  data-lhi="label_new" data-lh-lang>{{'n42ksv6tu6t.lh_sjlit0_new' | t }}</span>
										{%- break -%}
									{%- endif -%}
								{%- endfor -%}
