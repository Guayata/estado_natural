						<a href="{{item.url}}">
							 
								<img src="{{ item.image | img_url: '180x180', crop: 'center' }}" alt="Image">
							 
						</a>
					</div>
					<div class="lh-info">
						<h4 class="lh-title lh-full-text">
							<a href="{{item.url}}">{{item.title}}</a>
						</h4>
					<div class="lh-date">
						<a href="{{item.url}}">{{ item.created_at | date : format : 'abbreviated_date' }}</a>
					</div>
					</div>
				</div>
			</div>
			{% endfor %}</div>

			<div class="lh-btn"  data-lhi="btn" data-lh-lang data-lhi-child="a"><a href="https://estadonatural.com.mx/blogs/recetas-de-comidas-saludables" data-ga-action="tracking" data-ga-label="Button" data-lhlink>{{'qes2fii5lw.btn' | t }}</a></div>

