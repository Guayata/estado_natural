<section id="" data-section-id="vmf9zc90dh" data-section="blog-slider" data-lh-page-id="184" data-lh-page-name="Homepage - Coma" data-layouthub="section">  
 





 




<div class="lh-blog-block">

	<div class="lh-blog-wrapper">

		<div class="lh-container">

			<div class="lh-collection-heading">

				<h3 class="lh-collection-heading__title"  data-lhi="title" data-lh-lang>{{'vmf9zc90dh.title' | t }}</h3>

				<p class="lh-collection-heading__des"  data-lhi="des" data-lh-lang>{{'vmf9zc90dh.des_html' | t }}</p>

			</div>

			<div class="lh-blog-list">

				<div class="js-blog-list owl-carousel owl-theme">

					{% assign current = blogs['recetas-de-comidas-saludables'] %}
			 

			{% assign limit = 10 %}

			{% for item in current.articles limit:limit%}
				<div class="lh-blog-item">
					<div class="lh-blog--thumbnail">
						<a href="{{item.url}}">
							 
								<img src="{{ item.image | img_url: '300x300', crop: 'center' }}" alt="Image">
							 
						</a>
					</div>
					<div class="lh-blog--body">
						<div class="lh-body--meta">
							<span class="lh-body--meta__general lh-body--meta__date">
								<a href="{{item.url}}">{{ item.created_at | date : format : 'abbreviated_date' }}</a>
							</span>
							{% if item.comments_count > 0 %}
							<span class="lh-body--meta__general lh-body--meta__comment">
								<a href="{{item.url}}#comments">{{item.comments_count}} Comment</a>
							</span>
							{% endif %}
						</div>
						<div class="lh-body--header  ">
							<h3 class="lh-body--header__title full_text">
								<a href="{{item.url}}">{{item.title}}</a>
							</h3>
						</div>
						<!-- <div class="lh-body--content ">
							{% if item.excerpt.size > 0 %}
							<p>
								{{ item.excerpt | strip_html | truncate : 150 }}
							</p>
							{% else %}
							<p>
								{{ item.content | strip_html | truncate : 150 }}
							</p>
							{% endif %}
						</div> -->
						<!-- <div class="lh-body--action">
							<a href="{{item.url}}" class="lh-body--action__btn"  data-lhi="read_more"></a>
						</div> -->
					</div>
				</div>
			{% endfor %}</div>

			</div>



			<div class="lh-button"  data-lhi="cta" data-lh-lang data-lhi-child="a"><a href="/blogs/recetas-de-comidas-saludables" data-ga-action="tracking" data-ga-label="View more" data-lhlink>{{'vmf9zc90dh.cta' | t }}</a></div>



		</div>

	</div>

</div>



